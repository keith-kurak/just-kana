{
  "cli": {
    "version": ">= 3.2.1",
    "promptToConfigurePushNotifications": false,
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "resourceClass": "m1-medium"
      }
    },
    "development-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "autoIncrement": true,
      "channel": "preview",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "resourceClass": "m1-medium"
      },
      "env" : {
        "EXPO_PUBLIC_CODE_NAME": "neko"
      }
    },
    "preview-simulator": {
      "extends": "preview",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "ios": {
        "resourceClass": "m1-medium"
      },
      "env" : {
        "EXPO_PUBLIC_CODE_NAME": "kitsune",
        "EXPO_PUBLIC_TEST_VAL_A": "alpaca"
      }
    },
    "production-simulator": {
      "channel": "production",
      "autoIncrement": true,
      "ios": {
        "resourceClass": "m1-medium",
        "simulator": true
      }
    },
    "production-apk": {
      "extends": "production",
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
