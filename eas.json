{
  "cli": {
    "version": ">= 3.2.1",
    "promptToConfigurePushNotifications": false,
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "environment": "development",
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "DEV_VERSION": "1"
      }
    },
    "development:simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "autoIncrement": true,
      "channel": "preview",
      "environment": "preview",
      "android": {
        "buildType": "apk"
      }
    },
    "preview:simulator": {
      "extends": "preview",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "channel": "production",
      "environment": "production",
      "autoIncrement": true
    },
    "production:simulator": {
      "extends": "production",
      "channel": "production",
      "autoIncrement": true,
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production:apk": {
      "extends": "production",
      "android": {
        "buildType": "apk"
      }
    },
    "external:preview": {
      "extends": "production",
      "channel": "external-preview",
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {},
    "external-preview": {}
  }
}