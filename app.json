{
  "expo": {
    "name": "Some Kana",
    "slug": "just-kana",
    "owner": "pt-staging-org",
    "version": "1.3.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/icon.png",
    "backgroundColor": "#222222",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#222222"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.keithkurak.justkana",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#222222"
      },
      "package": "com.keithkurak.justkana"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "cfad7b35-7c3a-4952-993c-6e808af3a252"
      }
    },
    "updates": {
       "url": "https://staging-u.expo.dev/cfad7b35-7c3a-4952-993c-6e808af3a252"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.8.10"
          }
        }
      ]
    ]
  }
}